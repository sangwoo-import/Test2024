                    android:id="@+id/menu7"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:background="@drawable/imageviewshape"
                    android:elevation="4dp"
                    android:src="@drawable/check_small_resize" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="다면평가" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="top|center"
                android:orientation="vertical">

                <ImageButton
                    android:id="@+id/menu8"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:background="@drawable/imageviewshape"
                    android:elevation="4dp"
                    android:src="@drawable/settings_resize" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="설정" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="top|center"
                android:orientation="vertical">

                <ImageButton
                    android:id="@+id/menu13"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:background="@drawable/imageviewshape_enabled_false"
                    android:elevation="4dp"
                    android:src="@drawable/warninig_resize" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:text="작업중" />
            </LinearLayout>


        </LinearLayout>

